            <kind>1</kind>
            <string>if(!audio_is_playing(snd_music)) {
    audio_play_sound(snd_music, 24, true);
}
if(stop == false) {
//Setting up varibles
x_pre = x;
y_pre = y;
//Getting direction and Speed
f_speed = point_distance(x, y, mouse_x, mouse_y) / 4;
if(f_speed &gt; max_speed) {
    f_speed = max_speed;
}
f_direction = point_direction(x, y, mouse_x, mouse_y);

//Calculating how to move according to direction and speed
prex = f_speed * cos(f_direction*(pi/180));
prey = -(f_speed * sin(f_direction*(pi/180)));

max_x = prex;
max_y = prey;

if(max_x &lt; 0) {
    max_x = -(max_x);
}
if(max_y &lt; 0) {
    max_y = -(max_y);
}

//Adding the movement and avoiding solid objects
if(!place_free(x + prex, y + prey)) {
    move_contact_solid(f_direction, f_speed);
    while(x &lt; mouse_x) {
        if(place_free(x + 1, y)) {
            if(max_x &gt; 1) {
                x += 1;
                max_x--;
            } else {
                break;
            }
        } else {
            break;
        }
    }
    while(x &gt; mouse_x) {
        if(place_free(x - 1, y)) {
            if(max_x &gt; 1) {
                x -= 1;
                max_x--;
            } else {
                break;
            }
        } else {
            break;
        }
    }
    while(y &lt; mouse_y) {
        if(place_free(x, y + 1)) {
            if(max_y &gt; 1) {
                y += 1;
                max_y--;
            } else {
                break;
            }
        } else {
            break;
        }
    }
    while(y &gt; mouse_y) {
        if(place_free(x, y - 1)) {
            if(max_y &gt; 1) {
                y -= 1;
                max_y--;
            } else {
                break;
            }
        } else {
            break;
        }
    }
} else {
    x += prex;
    y += prey;
}
}

if(dash == true) {
    if(distance_to_point(mouse_x, mouse_y) &gt; 300) {
        max_speed = 64;
        stop = false;
        dash = false;
        alarm[0] = 30;
    }
}

if(x &gt; (view_xview + view_wview - 48)) {
    if(view_xview + view_wview &lt; room_width) {
        view_xview += view_wview;
        x += 96
    }
}

if(x &lt; view_xview + 48) {
    if(view_xview - view_wview &gt;= 0) {
        view_xview -= view_wview;
        x -= 96;
    }
}

if(y &gt; view_yview + view_hview - 48) {
    if(view_yview + view_hview &lt; room_height) {
        view_yview += view_hview;
        y += 96;
    }
}
if(y &lt; view_yview + 48) {
    if(view_yview - view_hview &gt;= 0) {
        view_yview -= view_hview;
        y -= 96;
    }
}
cooldown_index = alarm[1];

if(p_health &lt; 1) {
    visible = false;
    stop = true;
    if(dead == false) {
        dead = true;
        alarm[4] = 60;
        instance_create(x, y, obj_dead);
    }
}

audio_group_set_gain(sound, global.volume_sound / 10, 0);
